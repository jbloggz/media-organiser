{
  "name": "media-organiser",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve:client": "vue-cli-service serve",
    "serve:server": "nodemon --watch server --exec 'babel-node --presets babel-preset-env' server/server.js",
    "run": "npx babel-node --presets babel-preset-env dist/server.js",
    "build": "vue-cli-service build && npx babel --presets babel-preset-env -d dist server",
    "build-dev": "vue-cli-service build --mode development && npx babel --presets babel-preset-env -s -d dist server",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "clean": "rm -r dist"
  },
  "dependencies": {
    "@mdi/font": "^3.5.95",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "exif-parser": "^0.1.12",
    "express": "^4.16.4",
    "fluent-ffmpeg": "^2.1.2",
    "md5": "^2.2.1",
    "mime-types": "^2.1.22",
    "sharp": "^0.22.0",
    "sqlite3": "^4.0.6",
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "vuetify": "^1.5.5",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-e2e-cypress": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-unit-jest": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "fibers": "^3.1.1",
    "lodash": "^4.17.11",
    "nodemon": "^1.18.11",
    "sass": "^1.17.2",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.5.21",
    "vuetify-loader": "^1.0.5"
  }
}
